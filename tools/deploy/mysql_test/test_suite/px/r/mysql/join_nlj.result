create table stu (
sid     int,
name    varchar(32),
cls     int,
primary key (cls, sid)
) partition by hash(sid) partitions 6;
create table teacher (
tid        int,
name       varchar(32),
subject    varchar(4),
primary key (tid)
) partition by hash(tid) partitions 8;
create table score (
sid     int,
subject varchar(4),
score   int,
primary key (sid, subject)
) partition by hash(sid) partitions 6;
